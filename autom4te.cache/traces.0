                    want_ssl="no"
                elif test "$withval" = "yes"; then
                    want_ssl="yes"
                    ac_ssl_path=""
                else
                    want_ssl="yes"
                    ac_ssl_path="$withval"
                fi
                ],
                [want_ssl="yes"])

